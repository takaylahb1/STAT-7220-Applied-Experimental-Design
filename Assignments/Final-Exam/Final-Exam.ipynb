mod1 <- aov(Satisfaction ~ Protein * Salsa * Potatoes, data = burritos)
summary(mod1)

# Extract residuals
resid_mod1 <- residuals(mod1)

# Shapiro-Wilk test for normality
shapiro.test(resid_mod1)

# QQ plot
qqnorm(resid_mod1)
qqline(resid_mod1)

# Histogram of residuals
hist(resid_mod1, breaks = 10, main = "Residuals Histogram", xlab = "Residuals")

