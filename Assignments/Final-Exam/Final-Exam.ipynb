  }, ## PART 2 – QUESTION 12 ##
## Overall Conclusion Support Code ##

library(tidyverse)
library(emmeans)
library(effectsize)

# Load data
sales <- readxl::read_excel("Busters Burrito Sales.xlsx") %>%
  mutate(
    Location  = factor(Location),
    Timing    = factor(Timing),
    Marketing = factor(Marketing)
  )

# Fit main-effects model for fractional factorial design
mod2 <- aov(Sales ~ Location + Timing + Marketing, data = sales)
summary(mod2)

# Get mean sales for each factor level
mean_location  <- sales %>% group_by(Location)  %>% summarise(mean_sales = mean(Sales))
mean_timing    <- sales %>% group_by(Timing)    %>% summarise(mean_sales = mean(Sales))
mean_marketing <- sales %>% group_by(Marketing) %>% summarise(mean_sales = mean(Sales))

mean_location
mean_timing
mean_marketing

# Calculate main effects (difference between high and low)
effect_location  <- diff(mean_location$mean_sales)
effect_timing    <- diff(mean_timing$mean_sales)
effect_marketing <- diff(mean_marketing$mean_sales)

effect_location
effect_timing
effect_marketing

# Partial Eta Squared
eta_squared(mod2, partial = TRUE)

# Identify BEST overall combination by mean
best_combo <- sales %>%
  group_by(Location, Timing, Marketing) %>%
  summarise(mean_sales = mean(Sales)) %>%
  arrange(desc(mean_sales))

best_combo

